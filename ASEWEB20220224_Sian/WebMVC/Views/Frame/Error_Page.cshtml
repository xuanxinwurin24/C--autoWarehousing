    <style>

        .tabs {
            display: flex;
            flex-wrap: wrap;
            max-width: 1300px;
            max-height: 800px;
            font-family: sans-serif;
        }

        .tabs__label {
            padding: 10px 16px;
            cursor: pointer;
        }

        .tabs__radio {
            display: none;
        }

        .tabs__content {
            order: 1;
            width: 100%;
            height: 100%;
            border-bottom: 0px solid #dddddd;
            line-height: 1.5;
            font-size: 0.9em;
            display: none;
        }

        .tabs__radio:checked + .tabs__label {
            font-weight: bold;
            color: #009578;
            border-bottom: 2px solid #009578;
        }

            .tabs__radio:checked + .tabs__label + .tabs__content {
                display: initial;
            }
    </style>
</head>
