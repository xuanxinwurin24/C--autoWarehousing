<style>
    .top1 {
        width: 100%;
        height: 5%;
        background-color: rgb(43, 43, 43);
        position: absolute;
        top: 0%;
        left: 0%;
        color: white;
        font-size: 10%;
        line-height: 200%;
        font-family: Microsoft JhengHei;
        font-weight: bold;
        box-sizing: border-box;
    }

    .block1 {
        width: 100%;
        position: absolute;
        left: 10%;
        top: 5%;
        display: inline-block;
        z-index: -1;
    }

    .block2 {
        width: 100%;
        height: 80%;
        position: absolute;
        left: -10%;
        top: 100%;
        z-index: -1;
    }

    .block3 {
        width: 60%;
        height: 100%;
        position: absolute;
        right: 3%;
        bottom: -1250%;
        display: inline-block;
    }

    .background_left {
        width: 36%;
        height: 220px;
        position: absolute;
        bottom: -1740%;
        left: 5px;
        z-index: 1;
        background-color: aliceblue;
        display: inline-block;
    }

    .background_bottom {
        width: 60%;
        height: 50px;
        position: absolute;
        bottom: -1730%;
        left: -5px;
        z-index: 2;
        background-color: aliceblue;
    }

    .border {
        width: 180px;
        height: 130%;
        position: relative;
        display: inline-block;
        z-index: -1;
    }

    .top {
        position: relative;
        font-size: 25px;
        padding: 5px;
        float: left;
        margin: 5px;
        display: inline-block;
    }

    .button {
        width: 100px;
        height: 30px;
        font-size: 100%;
        background-color: rgb(51, 255, 0);
        left: 20px;
        position: absolute;
        bottom: -10%;
    }

    .button2 {
        width: 120%;
        height: 50%;
        font-size: 100%;
        background-color: rgb(51, 255, 0);
        margin: 10%;
        position: absolute;
        bottom: -10%;
    }

    .temp {
        position: absolute;
        top: 10px;
        left: 20px;
        font-size: 100%;
    }

    .percent {
        position: absolute;
        right: 60px;
        top: 10px;
        font-size: 100%;
    }

    .CarouselData_id {
        color: black;
        font-family: Microsoft JhengHei;
        font-weight: bold;
        font-size: 20px;
        padding: 1.5%;
        float: left;
    }

    .CarouselData {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        z-index: 1; /* Sit on top */
        left: 0;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
        padding-top: 60px;
    }

    .CarouselData-content {
        background-color: #fefefe;
        margin: 5% auto 15% auto; /* 5% from the top, 15% from the bottom and centered */
        border: 1px solid #888;
        width: 50%; /* Could be more or less, depending on screen size */
        height: 50%;
    }

    .CarouselData_border {
        width: 50%;
        height: 5%;
        border: 1px solid black;
        position: absolute;
        background-color: red;
        left: 25%;
    }

    .CarouselData_id + p {
        height: 20px;
        float: left;
        font-size: 20px;
        background-color: white;
        padding: 10px;
        line-height: 100%;
        position: relative;
        top: -50%;
    }
</style>
